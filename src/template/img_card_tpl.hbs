{{#each this}}
<div class="photo-card">
    <div class="wrap">
       <div class="thumb"><a href="{{largeImageURL}}"><img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" /></a></div>
            
        <div class="info">
            <p class="info-item">
                <b>Likes </b> <span class="">{{likes}}</span>
            </p>
            <p class="info-item">
                <b>Views </b>{{views}}
            </p>
            <p class="info-item"><b>Comments </b>{{comments}}</p>
            <p class="info-item">
                <b>Downloads </b>{{downloads}}
            </p>
         </div>
    </div>
 </div>
    
{{/each}}